package pt.inescid.l2f.xipapi.exception;

public class NodeHasNoParentPhraseException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public NodeHasNoParentPhraseException() {
	}

	public NodeHasNoParentPhraseException(String message) {
		super(message);
	}

	public NodeHasNoParentPhraseException(Throwable cause) {
		super(cause);
	}

	public NodeHasNoParentPhraseException(String message, Throwable cause) {
		super(message, cause);
	}

}
