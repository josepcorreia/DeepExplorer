package pt.inescid.l2f.xipapi.exception;

public class NodeHasNoSiblingsException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public NodeHasNoSiblingsException() {
	}

	public NodeHasNoSiblingsException(String message) {
		super(message);
	}

	public NodeHasNoSiblingsException(Throwable cause) {
		super(cause);
	}

	public NodeHasNoSiblingsException(String message, Throwable cause) {
		super(message, cause);
	}

}
