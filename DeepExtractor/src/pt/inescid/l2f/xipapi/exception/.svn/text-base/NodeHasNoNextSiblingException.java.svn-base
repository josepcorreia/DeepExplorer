package pt.inescid.l2f.xipapi.exception;
/**
 * 
 * @author Nuno Nobre
 *
 */
public class NodeHasNoNextSiblingException extends Exception {

	private static final long serialVersionUID = 1L;

	public NodeHasNoNextSiblingException() {
	}

	public NodeHasNoNextSiblingException(String message) {
		super(message);
	}

	public NodeHasNoNextSiblingException(Throwable cause) {
		super(cause);
	}

	public NodeHasNoNextSiblingException(String message, Throwable cause) {
		super(message, cause);
	}

}
